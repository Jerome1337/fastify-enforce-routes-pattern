{
  "name": "@jerome1337/fastify-enforce-routes-pattern",
  "version": "1.0.2",
  "description": "A Fastify plugin that enforces naming pattern for route paths",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "test": "node --test --require ts-node/register src/**/index.test.ts",
    "test:coverage": "c8 node --test --require ts-node/register src/**/index.test.ts",
    "test:watch": "node --test --watch --require ts-node/register src/**/index.test.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "fastify",
    "fastify-plugin",
    "snake_case",
    "kebab-case",
    "camelCase",
    "PascalCase",
    "routes",
    "naming-pattern"
  ],
  "author": "Jérôme Pogeant <hello@jeromepogeant.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jerome1337/fastify-enforce-routes-pattern.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/jerome1337/fastify-enforce-routes-pattern#readme",
  "bugs": {
    "url": "https://github.com/jerome1337/fastify-enforce-routes-pattern/issues"
  },
  "peerDependencies": {
    "fastify": ">=5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.31.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@types/node": "^22.16.5",
    "c8": "^10.1.3",
    "eslint": "^9.31.0",
    "fastify": "^5.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "semantic-release": "^24.2.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {
    "fastify-plugin": "^5.0.1"
  }
}
